<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.utunan.mapper.user.PublishQuizMapper">

    <select id="selectPublishQuiz" resultType="com.utunan.pojo.base.community.Quiz">
        select *
        from quiz q
        where q.userId=#{userId}
    </select>
    <select id="selectQuizTag" resultType="com.utunan.pojo.base.community.Tag">
        select t.*
        from tag t,quiz_tag qt
        where qt.quizId=#{quizId} and t.tagId=qt.tagId
    </select>

</mapper>